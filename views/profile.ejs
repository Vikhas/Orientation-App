<section class="features" id="features">

    <h1 class="heading">Hello <%- user.name %></h1>

    <div class="box-container">

        <div class="box">
            <!-- <img src="images/f-icon1.png" alt=""> -->
            <div class="heading">Details</div>
            
            <h3>Register No : <%- user.rollno %></h3>
           
            <h3>Department : <%- user.department %></h3>
            <h3>Batch No : <%- user.batch %></h3>
            
            <!-- <a href="#" class="btn">read more</a> -->
        </div>

        <div class="box">
            <div class="heading">Ongoing Session Details</div>
            <% sch.forEach(sh =>{ %>
            <% let starttime=sh.stime.split(":"); %>
            <% let endtime=sh.etime.split(":"); %>
            <% let startsec=parseInt(starttime[0] * 3600 + starttime[1] * 60); %>
            <% let endsec=parseInt(endtime[0] * 3600 + endtime[1] * 60); %>
            <% if(currsec>=startsec && currsec<=endsec){ %>
            <h3>Day No : <%= sh.dayno %></h3>
            <h3>Date : <%= sh.date %></h3>
            <h3>Session : <%= sh.ses %></h3>
            <h3>Time : <%= sh.stime %> to <%= sh.etime %></h3>
            <h3>Venue : <%= sh.venue %></h3>
            <h3>Staff Incharge : <%= sh.staffinc %></h3>
            <h3>Batch Incharge : <%= sh.batchinc %></h3>
            <% } %>
            <% }); %>
        
        </div>

      
    </div>

</section>
