        <header id="header" class="">
        
        
              <div class="logo" style="text-align: center;display: flex;justify-content: center;">
                
                    <img src="img/psg.png" alt="logo" />
                    <h1 class="text-light"><a href="index.html"><span>PSG Institute of Technology and Applied Research</span><br><span>Orientation 2022</span></a></h1>
                
                
              
                <!-- Uncomment below if you prefer to use an image logo -->
                
              </div>
        
              
          </header><!-- End Header -->
        






        <section class="h-100">
            <div class="container h-100">
                <div class="row justify-content-md-center h-100">
                    <div class="card-wrapper">
                        
                        <div class="card fat">
                            <div class="card-body">
                                <h4 class="card-title">Search Details</h4>
                                <form
                                    action="/search"
                                    method="POST"
                                    class="my-login-validation"
                                    novalidate=""
                                >
                                    <%- include ("./partials/message"); %>

                                    <div class="form-group">
                                        
                                        <b><label for="batchno">Batch Number:</label></b>
                                        <select name="batchno" id="batchno">
                                            <option value="A">A</option>
                                            <option value="B">B</option>
                                            <option value="C">C</option>
                                            <option value="D">D</option>
                                            <option value="E">E</option>
                                            <option value="F">F</option>
                                          </select>
                                        <br><br>
                                        
                                        <b><label for="date">Date:</label></b>
                                        <select name="date" id="date">
                                            <option value="0">27/10/2022</option>
                                            <option value="1">28/10/2022</option>
                                            <option value="2">29/10/2022</option>
                                            <option value="3">31/10/2022</option>
                                            <option value="4">01/11/2022</option>
                                            <option value="5">02/11/2022</option>
                                            <option value="6">03/11/2022</option>
                                            <option value="7">04/11/2022</option>
                                            <option value="8">05/11/2022</option>
                                          </select>
                                          
                                    </div>

                                    

                                    <div class="form-group m-0">
                                        <button
                                            type="submit"
                                            class="btn btn-primary btn-block"
                                        >
                                            Login
                                        </button>
                                    </div>
                                   
                                    
                                </form>
                            </div>
                        </div>
                        
                    </div>
                    <!-- new table -->

        
        <div class="container" style="text-align: center;height: 480px;margin-top: 12px;">
        
        <br>
        <footer style="position: fixed;left: 0;bottom: 0;width: 100%;color: white;text-align: center;">
            <div class="row" style="margin-right: 0px;">
        
                <div class="col-md-12 text-center" >
                  <p style="color:#281c1c;"> &copy; Copyright Â© 2022|CSE STUDENTS PSG INSTITUTE OF TECHNOLOGY AND APPLIED RESEARCH.<br />All Rights Reserved</p>
                </div>
              </div>

        </footer>
        </div>
        <!-- end of new table -->
                </div>
            </div>
            
            
        </section>
    
        


        <script
            src="https://code.jquery.com/jquery-3.3.1.slim.min.js"
