<section class="h-100">

<div class="container" style="text-align: center;height: 480px;margin-top: 12px;">
    <h2>Date: <%= sch[0].date %></h2>
    <h2>Day: <%= sch[0].day %></h2>
    <h2>Batch: <%= sch[0].batch %></h2>

<div class="card fat">

    <div class="table-responsive mt-2" style="box-shadow: 0 4px 8px rgb(0 0 0 / 5%);">
    
            <table class="table table-hover w-55">
                <thead class="table-success">
                    <tr>
                       
     
                        <th class="th-sm" colspan="1">Session</th>
                        <th class="th-sm" >Time</th>
                        <th class="th-sm" >Staff Incharge</th>
                        <th class="th-sm" >Venue</th>
                        
                        

                    </tr>
                </thead>
                <tbody>
                    <% sch.forEach(sh =>{ %>
                    <tr>
                        <td>
                            <span>
                                <h6><%= sh.ses %></h6>
                            </span>
                        </td>
                        <td>
                            <span>
                                <h6><%= sh.stime %> to <%= sh.etime %></h6>
                            </span>
                        </td>
                        <td>
                            <span>
                                <h6><%= sh.staffinc %></h6>
                            </span>
                        </td>
                        <td>
                            <span>
                                <h6><%= sh.venue %></h6>
                            </span>
                        </td>
                        
                    </tr>
                    <% }) %>
                   

                    
                </tbody>
            </table>
            



        
        
    </div>
</div>


